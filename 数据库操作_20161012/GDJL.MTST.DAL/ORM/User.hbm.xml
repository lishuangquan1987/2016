<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="GDJL.MTST.Model.User,GDJL.MTST.Model" table="User">
    <id name="UserName" type="String" unsaved-value="0">
      <column name="UserName" sql-type="VARCHAR" not-null="true" unique="true" />
      <generator class="assigned">
      </generator>
    </id>
    <property name="PassWord"  type="String">
      <column name="PassWord" length="50" sql-type="VARCHAR" not-null="true" unique="false"/>
    </property>
  </class>
</hibernate-mapping>
